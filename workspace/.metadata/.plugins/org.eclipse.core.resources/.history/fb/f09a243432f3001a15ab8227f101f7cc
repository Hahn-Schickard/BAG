    _builder.newLine();
    _builder.newLine();
    _builder.append("public class DeviceScanActivity extends ListActivity {");
    _builder.newLine();
    _builder.append("    ");
    _builder.append("private LeDeviceListAdapter mLeDeviceListAdapter;");
    _builder.newLine();
